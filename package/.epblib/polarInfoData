# polar bond define and the receptor charge with amber99SB
# Dependencies : opemmm, pdbfixer, openbabel, pybel

class Paras_info(object):

    def __init__(self, temp_illustration):
        # in order to record the paras information
        self.temp_illustration = temp_illustration

    def ligand_data(self):
        with open('ligand.database', 'w') as small:
            small.write(';%40s\n;  atom1  atom2       k             d\n;%40s\n' %(('='*40), ('='*40)))
            small.write('   C.1    N.1     3.11000       1.16000  \n')
            small.write('   C.ar    F      4.72000       1.36000  \n')
            small.write('   C.3     F      5.63000       1.33000  \n')
            small.write('   C.2     Cl     1.43000       1.82000  \n')
            small.write('   S.3     Cl     1.38000       2.12000  \n')
            small.write('   S.o2    Cl     1.36000       2.11000  \n')
            small.write('   C.ar    Cl     1.38000       1.72000  \n')
            small.write('   C.3     Cl     1.78000       1.78000  \n')
            small.write('   C.2     Br     1.18000       1.99000  \n')
            small.write('   S.3     Br     1.09000       2.36000  \n')
            small.write('   S.o2    Br     1.06000       2.37000  \n')
            small.write('   C.ar    Br     1.00000       1.90000  \n')
            small.write('   C.3     Br     1.33000       1.99000  \n')
            small.write('   C.2    O.2     3.51000       1.23000  \n')
            small.write('   C.2   O.co2    3.76000       1.25000  \n')
            small.write('   C.2    S.2     1.17000       1.66000  \n')
            small.write('   C.2     H      4.14000       1.09000  \n')
            small.write('   N.2    O.2     3.49000       1.21000  \n')
            small.write('   N.2     H      6.82000       1.01000  \n')
            small.write('   N.4     H     10.05000       1.01000  \n')
            small.write('   N.3     H      6.74000       1.01000  \n')
            small.write('   N.am    H      8.83000       1.01000  \n')
            small.write('  N.pl3    H      8.59000       1.01000  \n')
            small.write('  N.pl3  O.co2    3.80000       1.22000  \n')
            small.write('   O.3     H     10.97000       0.95000  \n')
            small.write('   S.3     H      3.29000       1.34000  \n')
            small.write('   S.3    O.2     2.44000       1.53000  \n')
            small.write('   S.o2   O.2     3.07000       1.47000  \n')
            small.write('   P.3   O.co2    3.92000       1.47000  \n')
            small.write(';%40s\n' %('='*40))
        with open(self.temp_illustration, 'a') as tmp_info:
            tmp_info.write('# The parameters of polar bond in small molecule.\n\t%s\n' %'ligand.database')

    def mmff94_charge(self):
        with open('mmff94.dat', 'w') as charge_file:
            charge_file.write(';%39s\n;  RESIDUE     ATOM    AMBER94.CHARGE\n;%39s\n' %(('='*39), ('-'*39)))
            charge_file.write('    ALA         N        -0.415700\n    ALA         H         0.271900\n')
            charge_file.write('    ALA         CA        0.033700\n    ALA         HA        0.082300\n')
            charge_file.write('    ALA         CB       -0.182500\n    ALA        HB1        0.060300\n')
