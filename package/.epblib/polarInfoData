        with open(self.temp_illustration, 'a') as tmp_info:
            tmp_info.write('# The parameters of residue charge information under force field(MMFF94).\n\t%s\n' %'mmff94.dat')


    def ligand_paras(self):
        self.ligand_data()
        sline = [_m.rstrip() for _m in open('ligand.database')]
        a0 = []; a1 = []; a2 = []
        for line in sline:
            if line[:1] != ';':
                a0.append(line.split()[0].strip()+line.split()[1].strip())
                a1.append(float(line.split()[2].strip()))
                a2.append(float(line.split()[3].strip()))
        return a0, a1, a2
